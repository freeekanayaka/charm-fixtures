#!/usr/bin/python3

import os
import sys


class ConfigTool():

    @classmethod
    def path(cls):
        return os.path.join(os.environ["CHARM_DIR"], ".config-get.data")

    def run(self, args):
        with open(self.path()) as fd:
            print(fd.read())


tool = ConfigTool()
tool.run(sys.argv[1:])
